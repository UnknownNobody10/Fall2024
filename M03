public class Population{
  public static void main(String[] args) {
    int time = 3600 * 24 * 365;
    int born = time / 7;
    int death = time / 13;
    int imm = time / 45;
    int current_pop = 312032486;
    int pop_change = born + imm - death;

    for (int i = 1; i == 5; i++) {
      current_pop = current_pop + pop_change;
      System.out.println(i + " year population is: " + current_population);
    }
    
  }
}
